# 1. Set the backend port to 3000
PORT=3000

# 2. Tell the backend where to redirect the user AFTER Google login
# This MUST match the frontend callback page we created
APP_REDIRECT_URL=http://localhost:5173/callback

# 3. Add your Supabase project URL and SECRET service key
# The SERVICE_KEY is secret and lives ONLY on the backend
SUPABASE_URL=YOUR_SUPABASE_URL_HERE
SUPABASE_SERVICE_KEY=YOUR_SUPABASE_SERVICE_KEY_HERE